// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Url {
  id            String    @id
  originUrl     String
  short_id      String    @unique
  createdAt     DateTime
  updatedAt     DateTime?
  deletedAt     DateTime?
  createdBy     String
  clickedAmount Int
}

model User {
  id        String   @id
  email     String   @unique
  password  String
  createdAt DateTime
}
